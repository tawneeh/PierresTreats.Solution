@{
  Layout = "_Layout";
}

<body class="splashImg">

  <h1>Pierre's Sweet & Savory Treats</h1>

  <h5>Treats</h5>
  @foreach (var treat in ViewBag.Treat)
  {
    <li id="splashList">@treat.Name</li>
  }
  <h5>Flavors</h5>
  @foreach (var flavor in ViewBag.Flavor)
  {
    <li id="splashList">@flavor.Name</li>
  }

</body>